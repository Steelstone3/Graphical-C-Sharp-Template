<UserControl
    x:Class="Example.Views.ClickMeView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>
        <StackPanel>
            <TextBlock Text="Enter some text:"/> 
            <TextBox Text="{Binding ClickMe.LatestResult.DiveProfileModel.DiveCeiling}"/>
            <Button
                Command="{Binding PrintText}" HotKey="Enter"
                FontWeight="Bold"
                Content="Click Me!" />
            <!-- IsEnabled="{Binding CanCalculateDiveStep}" -->
             
            <TextBlock Text="{Binding ClickMe.LatestResult.DiveProfileModel.DiveCeiling}" /> 
        </StackPanel>
    </Grid>
</UserControl>
